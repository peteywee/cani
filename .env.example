# Firebase Web SDK config (get these from Firebase Console > Project settings > SDK setup)
NEXT_PUBLIC_FIREBASE_API_KEY=your-api-key-here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=cani-dev-ws.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=cani-dev-ws
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=cani-dev-ws.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your-ga-id   # optional

# Emulator toggle
LOCAL_DEV=true

# Example Next.js app vars
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000/api
NEXTAUTH_SECRET=replace-with-random-string
NEXTAUTH_URL=http://localhost:3000
